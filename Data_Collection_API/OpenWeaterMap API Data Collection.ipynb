    "import json, requests\n",
    "\n",
    "db = []\n",
    "api_key = # type your api_key here\n"
    "\n",
    "# Make a connection to OpenWeatherMap API and obtain data for each zipcode \n",
    "for var in City_list[1:]:\n",
    "    url='http://api.openweathermap.org/data/2.5/weather?zip={},us&APPID={}'.format(var[0], api_key)\n",
    "    response = requests.request('GET', url)\n",
    "    p = json.loads(response.text)\n",
    "    dictionary = dict(p)\n",
    "    db.append(dictionary)"
   ]
