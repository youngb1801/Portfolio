   "source": [
    "from datetime import datetime\n",
    "\n",
    "# Convert temperature and date variables for each record\n",
    "for r in db:\n",
    "    num = db.index(r)\n",
    "    if 'dt' in r:\n",
    "        record_date = datetime.fromtimestamp(db[num]['dt']).strftime(\"%A, %B %d, %Y %I:%M:%S\")\n",
    "        db[num]['record_date'] = record_date\n",
    "    if 'sys' in r:\n",
    "        sunrise_date = datetime.fromtimestamp(db[num]['sys']['sunrise']).strftime(\"%A, %B %d, %Y %I:%M:%S\")\n",
    "        db[num]['sunrise_date'] = sunrise_date\n",
    "        sunset_date = datetime.fromtimestamp(db[num]['sys']['sunset']).strftime(\"%A, %B %d, %Y %I:%M:%S\")\n",
    "        db[num]['sunset_date'] = sunset_date\n",
    "    if 'main' in r:\n",
    "        temp_F = (((db[num]['main']['temp'])-273.15)*(9/5)+32)\n",
    "        db[num]['temp_F'] = round(temp_F, 2)\n",
    "        feels_like_F = (((db[num]['main']['feels_like'])-273.15)*(9/5)+32)\n",
    "        db[num]['feels_like_F'] = round(feels_like_F, 2)\n",
    "        temp_min_F = (((db[num]['main']['temp_min'])-273.15)*(9/5)+32)\n",
    "        db[num]['temp_min_F'] = round(temp_min_F, 2)\n",
    "        temp_max_F = (((db[num]['main']['temp_max'])-273.15)*(9/5)+32)\n",
    "        db[num]['temp_max_F'] = round(temp_max_F, 2)\n",
    "        pressure_milbar = float(db[num]['main']['pressure'])*10\n",
    "        db[num]['pressure_milbar'] = pressure_milbar\n",
    "\n",
    "print(db[1])"
   ]
